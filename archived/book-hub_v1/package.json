{
  "name": "book-hub",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "lint": "next lint --fix",
    "ts": "tsc --noEmit",
    "build": "next build",
    "build:analyze": "ANALYZE=true next dev",
    "build:storybook": "build-storybook",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only -P nexus.tsconfig.json pages/api",
    "generate:prisma&nexus": "npm -s run generate:prisma && npm -s run generate:nexus",
    "gererate:GraphqQL": "graphql-codegen --config codegen.yml",
    "gererate:GraphqQL:watch": "graphql-codegen --config codegen.yml --watch",
    "database:migrate": "npx prisma migrate dev --preview-feature",
    "database:reset": "npx prisma migrate reset --force --preview-feature",
    "prisma:studio": "npx prisma studio",
    "storybook": "start-storybook -s ./public -p 6006"
  },
  "dependencies": {
    "@apollo/client": "3.4.16",
    "@headlessui/react": "1.4.1",
    "@heroicons/react": "1.0.5",
    "@next-auth/prisma-adapter": "1.0.1",
    "@radix-ui/react-avatar": "0.1.1",
    "@radix-ui/react-dropdown-menu": "0.1.1",
    "@radix-ui/react-id": "0.1.5",
    "@radix-ui/react-progress": "0.1.3",
    "@radix-ui/react-switch": "0.1.4",
    "@splitbee/web": "0.3.0",
    "@tailwindcss/line-clamp": "0.3.1",
    "@tanem/react-nprogress": "4.0.8",
    "@use-gesture/react": "^10.2.4",
    "apollo-server-micro": "2.19.2",
    "classnames": "2.3.1",
    "date-fns": "2.28.0",
    "express": "4.17.3",
    "graphql": "15.5.1",
    "graphql-iso-date": "3.6.1",
    "iso-639-1": "2.1.13",
    "next": "12.1.4",
    "next-auth": "4.0.5",
    "next-pwa": "5.4.4",
    "nexus": "1.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "7.19.5",
    "react-hot-toast": "2.2.0",
    "react-markdown": "7.1.0",
    "react-spring": "9.4.3",
    "slugify": "1.6.5",
    "zustand": "3.7.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/named-operations-object": "2.2.1",
    "@graphql-codegen/typescript": "2.3.0",
    "@graphql-codegen/typescript-operations": "2.3.4",
    "@graphql-codegen/typescript-react-apollo": "3.2.5",
    "@next/bundle-analyzer": "12.0.10",
    "@prisma/client": "3.6.0",
    "@storybook/addon-a11y": "6.4.19",
    "@storybook/addon-essentials": "6.3.12",
    "@storybook/addon-links": "6.4.20",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/react": "6.3.12",
    "@types/graphql-iso-date": "3.4.0",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.6",
    "@types/react": "17.0.37",
    "@types/tailwindcss": "3.0.7",
    "autoprefixer": "10.4.0",
    "babel-loader": "8.2.3",
    "bundlemon": "1.1.1",
    "chromatic": "6.0.5",
    "lint-config": "workspace:*",
