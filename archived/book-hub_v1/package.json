    "storybook-css-modules-preset": "1.1.1",
    "tailwindcss": "3.0.18",
    "ts-node": "10.4.0",
    "tsconfig": "workspace:*",
    "typescript": "^4.7.4"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
