{
  "baseDir": "./.next",
  "files": [
    {
      "path": "static/chunks/pages/**/*-<hash>.js"
    },
    {
      "path": "static/**/framework-<hash>.js"
    },
    {
      "path": "static/**/main-<hash>.js"
    },
    {
      "path": "static/**/polyfills-<hash>.js"
    },
    {
      "path": "static/**/webpack-<hash>.js"
    }
  ],
  "groups": [
    {
      "path": "**/*.js"
    },
    {
      "path": "**/*.css"
    }
  ],
  "reportOutput": [
    [
      "github",
      {
        "checkRun": false,
        "commitStatus": true,
        "prComment": true
      }
    ]
  ],
  "verbose": true
}
