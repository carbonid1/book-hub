{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "build:book-hub": {
      "dependsOn": [
        "^build",
        "$NODE_ENV",
        "$GOOGLE_AUTH_ID",
        "$GOOGLE_AUTH_SECRET",
        "$GITHUB_AUTH_ID",
        "$TWITTER_AUTH_ID",
        "$TWITTER_AUTH_SECRET",
        "$NEXT_PUBLIC_VERCEL_ENV",
        "$NEXT_PUBLIC_VERCEL_URL",
        "$GITHUB_AUTH_SECRET"
      ],
      "outputs": [".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}
