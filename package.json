{
  "name": "book-hub",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "test": "jest",
    "dev": "next dev",
    "ts": "tsc --noEmit",
    "analyze": "ANALYZE=true next dev",
    "storybook": "start-storybook -s ./public -p 6006",
    "studio": "npx prisma studio",
    "migrate_db": "npx prisma migrate dev --preview-feature",
    "reset_db": "npx prisma migrate reset --force --preview-feature",
    "nexus_gen": "npm -s run generate:prisma && npm -s run generate:nexus",
    "graphql_gen": "graphql-codegen --config codegen.yml",
    "upgrade_storybook": "npm i -D @storybook/addon-postcss@latest @storybook/react@latest @storybook/addon-links@latest @storybook/addon-essentials@latest @storybook/addon-a11y@latest",
    "✋___comment__✋": "internal scripts ⏬",
    "build": "next build",
    "start": "next start",
    "build-storybook": "build-storybook",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only -P nexus.tsconfig.json pages/api",
    "prepare": "husky install",
    "lint": "next lint"
  },
  "dependencies": {
    "@apollo/client": "^3.4.8",
    "@headlessui/react": "^1.2.0",
    "@heroicons/react": "^1.0.1",
    "@splitbee/web": "^0.2.4",
    "@tailwindcss/line-clamp": "^0.2.1",
    "apollo-server-micro": "^2.19.2",
    "classnames": "^2.3.1",
    "date-fns": "^2.19.0",
    "express": "^4.17.1",
    "google-auth-library": "^7.6.2",
    "graphql": "^15.5.1",
    "graphql-iso-date": "^3.6.1",
    "iso-639-1": "^2.1.8",
    "next": "^11.1.0",
    "next-pwa": "^5.0.4",
    "nexus": "^1.0.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-google-login": "^5.2.2",
    "react-markdown": "^5.0.3",
    "react-spring": "^9.2.4",
    "react-use-gesture": "^9.1.3",
    "slugify": "^1.4.6",
    "ts-node": "^9.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-typescript": "^7.13.0",
    "@graphql-codegen/cli": "2.0.1",
    "@graphql-codegen/introspection": "2.0.0",
    "@graphql-codegen/typescript": "2.0.0",
    "@graphql-codegen/typescript-operations": "2.0.1",
    "@graphql-codegen/typescript-react-apollo": "3.0.0",
    "@next/bundle-analyzer": "^10.0.5",
    "@prisma/client": "^2.28.0",
    "@storybook/addon-a11y": "^6.3.6",
    "@storybook/addon-essentials": "^6.3.6",
    "@storybook/addon-links": "^6.3.6",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^6.3.6",
    "@types/classnames": "^2.3.1",
    "@types/graphql-iso-date": "^3.4.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^14.14.22",
    "@types/react": "^17.0.0",
    "@types/tailwindcss": "^2.2.1",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.2.2",
    "bundlemon": "^1.0.0",
    "chromatic": "^5.9.0",
    "eslint": "^7.32.0",
    "eslint-config-next": "^11.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.24.0",
    "husky": "^6.0.0",
    "jest": "^27.0.4",
    "lint-staged": "^11.0.0",
    "postcss": "^8.2.14",
    "prettier": "2.3.0",
    "prisma": "^2.28.0",
    "tailwindcss": "^2.1.2",
    "typescript": "^4.3.5"
  },
  "lint-staged": {
    "*.ts": "jest --passWithNoTests --findRelatedTests --bail",
    "*.{ts, tsx}": "eslint",
    "**/*": "prettier --write --ignore-unknown"
  }
}
